{
  "name": "rust-stuco-website",
  "description": "Website for the Rust StuCo (98-008)",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "run-s build:*",
    "build:fork": "run-s build:fork:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i assets/css/main.css -o _site/assets/css/main.css --watch",
    "build:11ty": "eleventy",
    "build:css": "tailwindcss -i assets/css/main.css -o _site/assets/css/main.css",
    "build:fork:11ty": "eleventy --pathprefix=/rust-stuco.github.io/",
    "build:fork:css": "tailwindcss -i assets/css/main.css -o _site/rust-stuco.github.io/assets/css/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rust-stuco/rust-stuco.github.io.git"
  },
  "author": "jurplel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rust-stuco/rust-stuco.github.io/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@kamona/tailwindcss-perspective": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "markdown-it-attrs": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "caniuse-lite": "^1.0.30001743"
  }
}
